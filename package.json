{
  "name": "restaurant-client",
  "version": "1.0.0",
  "description": "Client React Application",
  "main": "index.js",
  "scripts": {
    "dev": "NODE_PATH=./lib pm2 start lib/server.js --watch --interpreter babel-node -name",
    "dev-stop": "pm2 stop lib/server.js",
    "dev-logs": "pm2 logs",
    "webpack": "webpack -dw",
    "test": "jest --watch",
    "build-webpack": "webpack -p",
    "build-node": "babel lib -d build --copy-files",
    "start-prod": "node build/server.js"
  },
  "engines": {
    "node": "8.10.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zoharngo/restaurant-client.git"
  },
  "author": "Zohar Nyego",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/zoharngo/restaurant-client/issues"
  },
  "homepage": "https://github.com/zoharngo/restaurant-client#readme",
  "babel": {
    "presets": [
      "react",
      [
        "env",
        {
          "targets": {
            "node": "current"
          }
        }
      ]
    ],
    "plugins": [
      "transform-class-properties",
      "transform-object-rest-spread",
      "emotion"
    ]
  },
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^6.10.3",
    "jest": "^19.0.2",
    "react-test-renderer": "^16.6.0",
    "webpack": "^2.7.0"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-cli": "^6.24.1",
    "babel-loader": "^7.0.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-env": "^1.4.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "ejs": "^2.5.6",
    "express": "^4.15.2",
    "pm2": "^5.3.1",
    "prop-types": "^15.6.2",
    "react": "^16.6.0",
    "react-dom": "^16.6.0",
    "react-router-dom": "^4.3.1",
    "react-spinners": "^0.4.7"
  }
}
